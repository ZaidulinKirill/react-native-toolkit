{
  "name": "@kirz/react-native-toolkit",
  "version": "0.1.2-beta.5",
  "files": [
    "react-native.config.js",
    "lib",
    "scripts",
    "sentry",
    "firebase-remote-config"
  ],
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "import": "./lib/module/index.js",
      "default": "./lib/commonjs/index.js"
    },
    "./sentry": {
      "types": "./lib/typescript/plugins/Sentry/index.d.ts",
      "import": "./lib/module/plugins/Sentry/index.js",
      "default": "./lib/commonjs/plugins/Sentry/index.js"
    },
    "./firebase-remote-config": {
      "types": "./lib/typescript/plugins/FirebaseRemoteConfig/index.d.ts",
      "import": "./lib/module/plugins/FirebaseRemoteConfig/index.js",
      "default": "./lib/commonjs/plugins/FirebaseRemoteConfig/index.js"
    }
  },
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build && npx tsc-alias -p tsconfig.main.json && npx tsc-alias -p tsconfig.module.json && npx tsc-alias -p tsconfig.typescript.json",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": "~18.1.0",
    "react-native": "~0.70.6",
    "react-native-bootsplash": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/runtime": "^7.12.5",
    "@react-native-firebase/analytics": "^14.12.0",
    "@react-native-firebase/app": "^14.12.0",
    "@react-native-firebase/remote-config": "^14.12.0",
    "@sentry/integrations": "^7.28.1",
    "@sentry/react-native": "^5.0.0-alpha.10",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-consistent-default-export-name": "^0.0.15",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.1.0",
    "react-native": "^0.70.6",
    "react-native-bootsplash": "^4.4.0",
    "react-native-builder-bob": "^0.20.3",
    "react-native-fbsdk-next": "^11.1.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.8.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "react-native-dropdownalert": "^4.5.1",
    "use-async-effect": "^2.2.7"
  }
}
